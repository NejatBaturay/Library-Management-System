@model string
@{
    ViewBag.Title = "Giris";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@Model</h2>

@using(Html.BeginForm("Giris","Home",FormMethod.Post)){
<form>
    <div class="form-group">
        <label for="exampleInputEmail1">SicilNo</label>
        <input type="text" class="form-control" name="SicilNo" placeholder="Sici No Giriniz">
     
    </div>
    <div class="form-group">
        <label for="exampleInputPassword1">Sifre</label>
        <input type="password" class="form-control" name="sifre" placeholder="Sifre Giriniz">
    </div>
   
    <button type="submit" class="btn btn-primary">Giris</button>
</form>
}
